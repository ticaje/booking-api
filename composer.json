    "license": [
        "GPL-3.0"
    ],
    "description": "Hexagonal Design based PHP middleware for defining Booking Use Cases given any PHP based solution",
    "type": "library",
    "keywords": [
        "Booking Product",
        "PHP Hexagonal Design",
        "Use Case based API"
    ],

    "require": {
        "php": "^7.1",
        "ticaje/design-by-contract": "^1.0",
        "ticaje/hexagonal": "^1.0"
    },
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "Ticaje\\BookingApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ticaje\\BookingApi\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.2.0",
        "friendsofphp/php-cs-fixer": "~2.14.0",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "~3.4.0"
    }
}
